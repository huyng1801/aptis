{
  "name": "aptis-backend",
  "version": "1.0.0",
  "description": "APTIS - English Learning Platform Backend API with AI Integration",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:auth": "cross-env NODE_ENV=test jest --testPathPattern=auth --verbose --detectOpenHandles --forceExit",
    "test:admin": "cross-env NODE_ENV=test jest --testPathPattern=admin --verbose --detectOpenHandles --forceExit",
    "test:users": "cross-env NODE_ENV=test jest --testPathPattern=users --verbose --detectOpenHandles --forceExit",
    "test:exams": "cross-env NODE_ENV=test jest --testPathPattern=exams --verbose --detectOpenHandles --forceExit",
    "test:questions": "cross-env NODE_ENV=test jest --testPathPattern=questions --verbose --detectOpenHandles --forceExit",
    "test:student": "cross-env NODE_ENV=test jest --testPathPattern=student --verbose --detectOpenHandles --forceExit",
    "test:ai": "cross-env NODE_ENV=test jest --testPathPattern=ai --verbose --detectOpenHandles --forceExit",
    "test:debug": "node debug-auth.js",
    "test:reports": "cross-env NODE_ENV=test jest --testPathPattern=reports --verbose --detectOpenHandles --forceExit",
    "test:rubrics": "cross-env NODE_ENV=test jest --testPathPattern=rubrics --verbose --detectOpenHandles --forceExit",
    "test:system": "cross-env NODE_ENV=test jest --testPathPattern=system --verbose --detectOpenHandles --forceExit",
    "test:teacher-review": "cross-env NODE_ENV=test jest --testPathPattern=teacher-review --verbose --detectOpenHandles --forceExit",
    "migrate": "node src/scripts/migrate.js",
    "seed": "node src/scripts/seed.js",
    "lint": "eslint src/**/*.js"
  },
  "keywords": [
    "aptis",
    "english",
    "learning",
    "api",
    "gemini-ai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/speech": "^7.2.1",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "sequelize": "^6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
